/** * 交易数据 */CREATE TABLE finance.stock_trade_data (  trade_date varchar(10),	symbol varchar(6) ,	name varchar(20) ,	opening_price float,	last_trade float,	closing_price float,	change_rate float,	change_amount float,	volume float,	turnover float,	highest float,	lowest float,	amplitude float)WITH (OIDS=FALSE);ALTER TABLE finance.stock_trade_data OWNER TO postgres;COMMENT ON TABLE finance.stock_trade_data IS '交易数据';COMMENT ON COLUMN finance.stock_trade_data.trade_date IS '交易日期';COMMENT ON COLUMN finance.stock_trade_data.symbol IS '证券代碼';COMMENT ON COLUMN finance.stock_trade_data.name IS '证券名称';COMMENT ON COLUMN finance.stock_trade_data.opening_price IS '开盘价';COMMENT ON COLUMN finance.stock_trade_data.last_trade IS '最新价';COMMENT ON COLUMN finance.stock_trade_data.closing_price IS '收盘价';COMMENT ON COLUMN finance.stock_trade_data.change_rate IS '涨跌幅';COMMENT ON COLUMN finance.stock_trade_data.change_amount IS '涨跌额';COMMENT ON COLUMN finance.stock_trade_data.volume IS '成交量';COMMENT ON COLUMN finance.stock_trade_data.turnover IS '成交额';COMMENT ON COLUMN finance.stock_trade_data.highest IS '最高价';COMMENT ON COLUMN finance.stock_trade_data.lowest IS '最低价';COMMENT ON COLUMN finance.stock_trade_data.amplitude IS '// 振幅';